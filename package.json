{
  "name": "codefresh",
  "version": "1.0.0",
  "author": "Codefresh",
  "homepage": "https://codefresh.io/",
  "private": true,
  "browserslist": [
    "extends @wordpress/browserslist-config"
  ],
  "scripts": {
    "build": "mix",
    "build:production": "mix --production",
    "start": "mix watch",
    "hot": "mix watch --hot",
    "clear": "wp acorn optimize:clear",
    "test": "npm run lint",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint resources/js",
    "lint:css": "stylelint \"resources/**/*.{css,scss,vue}\"",
    "translate": "npm run translate:pot && npm run translate:js",
    "translate:pot": "wp i18n make-pot . ./resources/lang/aviate.pot --ignore-domain --include=\"app,resources\" --exclude=\"app/mu-plugins\"",
    "translate:js": "wp i18n make-json ./resources/lang --pretty-print"
  },
  "engines": {
    "node": ">=12.14.0"
  },
  "devDependencies": {
    "@tinypixelco/laravel-mix-wp-blocks": "^1.1.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@wordpress/babel-preset-default": "^5.0.0",
    "@wordpress/browserslist-config": "^3.0.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.3.0",
    "eslint": "^7.18.0",
    "eslint-plugin-import": "^2.22.1",
    "html-critical-webpack-plugin": "^2.1.0",
    "laravel-mix": "^6.0.10",
    "laravel-mix-bundle-analyzer": "^1.0.5",
    "laravel-mix-critical": "^0.1.2",
    "postcss": "^8.1",
    "sass": "^1.32.5",
    "sass-loader": "10.1.1",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "tailwind-config-viewer": "^1.6.2",
    "tailwindcss": "^2.0.2",
    "tailwindcss-aspect-ratio": "^3.0.0",
    "vue-loader": "^16.1.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.15.3",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.0",
    "clipboard": "^2.0.8",
    "downloadjs": "^1.4.7",
    "flickity": "^2.2.2",
    "hamburgers": "^1.1.3",
    "ion-rangeslider": "^2.3.1",
    "jquery": "^3.6.0",
    "jquery-bridget": "^3.0.0",
    "lazysizes": "^5.3.1",
    "lodash-es": "^4.17.21",
    "lottie-web": "^5.7.7",
    "numeral": "^2.0.6",
    "popper.js": "^1.16.1",
    "prismjs": "^1.23.0",
    "smooth-scroll": "^16.1.3",
    "video.js": "^7.11.4",
    "vue": "^3.0.11",
    "yarn": "^1.22.17"
  }
}
